            <StackPane fx:id="stackPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="1024.0">
            </StackPane>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="datosJugador" prefHeight="1080.0" prefWidth="920.0">
         <children>
            <VBox fx:id="vBoxDatos" alignment="TOP_CENTER" prefHeight="572.0" prefWidth="352.0" style="-fx-padding: 10px; -fx-spacing: 10px;" />
            <AnchorPane fx:id="botonera" layoutY="553.0" prefHeight="150.0" prefWidth="352.0">
            </AnchorPane>
             <ImageView fx:id="configuracion" fitHeight="40.0" fitWidth="200.0" layoutX="415.0" layoutY="10.0" onMouseClicked="#configuracion" pickOnBounds="true" preserveRatio="true" translateX="435.0">
                 <image>
                     <Image url="@../images/botonConfiguracionMadera.png" />
                 </image>
             </ImageView>
         </children>
      </AnchorPane>

</HBox>
